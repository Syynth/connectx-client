React = require 'react'

{
  Route,
  RouteHandler,
  DefaultRoute,
  NotFoundRoute
} = require 'react-router'

{
  App,
  Inbox,
  pages: {
    Clubhouse,
    Community,
    Course,
    Event,
    Group,
    Home,
    User,
    Search,
    Static
  }
} = require 'connectx/components'

module.exports =
  <Route handler={App}>
    <NotFoundRoute handler={Static.NotFound} />
    <Route name="home" handler={Home.Page} />
    <Route name="clubhouse" handler={Clubhouse.Page}>
      <DefaultRoute handler={Clubhouse.Main} />
      <Route name="clubhouse.friends"  path="friends"  handler={Clubhouse.Friends} />
      <Route name="clubhouse.groups"   path="groups"   handler={Clubhouse.Groups} />
      <Route name="clubhouse.events"   path="events"   handler={Clubhouse.Events} />
      <Route name="clubhouse.courses"  path="courses"  handler={Clubhouse.Courses} />
      <Route name="clubhouse.about"    path="about"    handler={Clubhouse.About} />
      <Route name="clubhouse.photos"   path="photos"   handler={Clubhouse.Photos} />
      <Route name="clubhouse.settings" path="settings" handler={Clubhouse.Settings} />
      <Route name="clubhouse.messages" path="messages" handler={Clubhouse.Messages} />
    </Route>
    <Route name="community" path="community" handler={Community} />
    <Route name="course" path="course/:id" handler={Course.Page}>
      <DefaultRoute handler={Course.Main} />
      <Route name="course.events"   path="events"   handler={Course.Events} />
      <Route name="course.groups"   path="groups"   handler={Course.Groups} />
      <Route name="course.location" path="location" handler={Course.Location} />
      <Route name="course.members"  path="members"  handler={Course.Members} />
      <Route name="course.messages" path="messages" handler={Course.Messages} />
      <Route name="course.photos"   path="photos"   handler={Course.Photos} />
      <Route name="course.settings" path="settings" handler={Course.Settings} />
    </Route>
    <Route name="user" path="user/:id" handler={User.Page}>
      <DefaultRoute handler={User.Main} />
      <Route name="user.friends" path="friends" handler={User.Friends} />
      <Route name="user.groups"  path="groups"  handler={User.Groups} />
      <Route name="user.courses" path="courses" handler={User.Courses} />
      <Route name="user.about"   path="about"   handler={User.About} />
      <Route name="user.photos"  path="photos"  handler={User.Photos} />
    </Route>
    <Route name="group" path="group/:id" handler={Group.Page}>
      <DefaultRoute handler={Group.Main} />
      <Route name="group.about"    path="about"    handler={Group.About} />
      <Route name="group.members"  path="friends"  handler={Group.Friends} />
      <Route name="group.messages" path="messages" handler={Group.Messages} />
      <Route name="group.times"    path="times"    handler={Group.Times} />
      <Route name="group.results"  path="results"  handler={Group.Results} />
      <Route name="group.photos"   path="photos"   handler={Group.Photos} />
      <Route name="group.settings" path="settings" handler={Group.Settings} />
    </Route>
    <Route name="event" path="event/:id" handler={Event.Page}>
      <DefaultRoute handler={Event.Main} />
      <Route name="event.attending" path="attending" handler={Event.Attending} />
      <Route name="event.invite"    path="invite"    handler={Event.Invite} />
      <Route name="event.photos"    path="photos"    handler={Event.Photos} />
      <Route name="event.results"   path="results"   handler={Event.Results} />
      <Route name="event.settings"  path="settings"  handler={Event.Settings} />
      <Route name="event.times"     path="times"     handler={Event.Times} />
    </Route>
    <Route name="search" handler={Search.Page}>
      <Route name="search.all"    path=":query"        handler={Search.All} />
      <Route name="search.user"   path="user/:query"   handler={Search.User} />
      <Route name="search.course" path="course/:query" handler={Search.Course} />
      <Route name="search.group"  path="group/:query"  handler={Search.Group} />
      <Route name="search.event"  path="event/:query"  handler={Search.Event} />
    </Route>
    <Route name="static" path="pages" handler={Static.Page}>
      <DefaultRoute handler={Static.About} />
      <Route name="static.about"     path="about"     handler={Static.About} />
      <Route name="static.advertise" path="advertise" handler={Static.Advertise} />
      <Route name="static.blog"      path="blog"      handler={Static.Blog} /> {### TODO: Add subroutes for individual posts ###}
      <Route name="static.contact"   path="contact"   handler={Static.Contact} />
      <Route name="static.faq"       path="faq"       handler={Static.Faq} />
      <Route name="static.info"      path="info"      handler={Static.Info} />
      <Route name="static.media"     path="media"     handler={Static.Media} />
      <Route name="static.notfound"  path="notfound"  handler={Static.NotFound} />
      <Route name="static.privacy"   path="privacy"   handler={Static.Privacy} />
      <Route name="static.terms"     path="terms"     handler={Static.Terms} />
    </Route>
  </Route>
