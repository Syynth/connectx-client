  pages: {
    Home,
    Clubhouse,
    ClubhouseMain,
    ClubhouseFriends
  }
} = require 'connectx/components'

module.exports =
  <Route handler={App}>
    <DefaultRoute handler={Home} />
    <Route name="home" handler={Home} />
    <Route name="clubhouse" handler={Clubhouse}>
      <DefaultRoute handler={ClubhouseMain} />
      <Route name="friends" handler={ClubhouseFriends} />
      <Route name="groups" handler={Clubhouse} />
      <Route name="courses" handler={Clubhouse} />
      <Route name="about" handler={Clubhouse} />
      <Route name="photos" handler={Clubhouse} />
      <Route name="settings" handler={Clubhouse} />
    </Route>

  </Route>
