React = require 'react'

{
  Route,
  RouteHandler,
  DefaultRoute
} = require 'react-router'

{
  App,
  pages: {
    Home,
    Clubhouse,
    ClubhouseMain,
    ClubhouseFriends,
    ClubhouseAbout,
    ClubhouseCourses,
    ClubhouseGroups,
    ClubhousePhotos,
    ClubhouseSettings,
    ClubhouseMessages
  }
} = require 'connectx/components'

module.exports =
  <Route handler={App}>
    <DefaultRoute handler={Home} />
    <Route name="home" handler={Home} />
    <Route name="clubhouse" handler={Clubhouse}>
      <DefaultRoute handler={ClubhouseMain} />
      <Route name="friends" handler={ClubhouseFriends} />
      <Route name="groups" handler={ClubhouseGroups} />
      <Route name="courses" handler={ClubhouseCourses} />
      <Route name="about" handler={ClubhouseAbout} />
      <Route name="photos" handler={ClubhousePhotos} />
      <Route name="settings" handler={ClubhouseSettings} />
      <Route name="messages" handler={ClubhouseMessages} />
    </Route>

  </Route>
