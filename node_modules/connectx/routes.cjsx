React = require 'react'

{
  Route,
  RouteHandler,
  DefaultRoute
} = require 'react-router'

{
  App,
  pages: {
    Home,
    Clubhouse,
    CSubs
  }
} = require 'connectx/components'

module.exports =
  <Route handler={App}>
    <DefaultRoute handler={Home} />
    <Route name="home" handler={Home} />
    <Route name="clubhouse" handler={Clubhouse}>
      <DefaultRoute handler={CSubs.Main} />
      <Route name="friends" handler={CSubs.Friends} />
      <Route name="groups" handler={CSubs.Groups} />
      <Route name="courses" handler={CSubs.Courses} />
      <Route name="about" handler={CSubs.About} />
      <Route name="photos" handler={CSubs.Photos} />
      <Route name="settings" handler={CSubs.Settings} />
      <Route name="messages" handler={CSubs.Messages} />
    </Route>

  </Route>
