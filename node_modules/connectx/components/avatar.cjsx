
React = require 'react'

{ImageUpload} = require 'connectx/components/file'

style =
  width: '50%'
  float: 'left'
  display: 'block'

module.exports = React.createClass
  render: ->
    <div>
      <div style={style}>
        <h3>Profile picture</h3>
        <ImageUpload {...@props} fieldName="profileImage" />
      </div>
      <div style={style}>
        <h3>Cover picture</h3>
        <ImageUpload {...@props} fieldName="coverImage" />
      </div>
    </div>
