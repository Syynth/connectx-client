# @cjsx React.DOM

React = require 'react/addons'
_ = require 'lodash'
{ModalTrigger} = require 'react-bootstrap'

AddButton = module.exports = React.createClass
  propTypes:
    form: React.PropTypes.element.isRequired
  render: ->
    <div className="add_btn wrapper clearfix">
      <ModalTrigger modal={@props.form}>
        <div className="button green left_float">{@props.children}</div>
      </ModalTrigger>
    </div>
