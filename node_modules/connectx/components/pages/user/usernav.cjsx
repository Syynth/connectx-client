# @cjsx React.DOM

React = require 'react'
{Link, State} = require 'react-router'
Icon = require 'connectx/components/icon'

UserNav = module.exports = React.createClass
  mixins: [State]
  render: ->
    <article className="nav_menu">
      <Link to="clubhouse" className="img">
        <figure>
          <img src="img/icons/golf_nav.png" />
        </figure>
      </Link>
      <Link to="user" params={id: @props.entity.id}><Icon fa-user />Profile</Link>
      <Link to="user.friends" params={id: @props.entity.id}>Friends</Link>
      <Link to="user.groups" params={id: @props.entity.id}>Groups</Link>
      <Link to="user.courses" params={id: @props.entity.id}>Courses</Link>
      <Link to="user.about" params={id: @props.entity.id}>About</Link>
      <Link to="user.photos" params={id: @props.entity.id}>Photos</Link>
    </article>
