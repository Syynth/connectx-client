# @cjsx React.DOM

React = require 'react'
{Link, State} = require 'react-router'
Icon = require 'connectx/components/icon'

UserNav = module.exports = React.createClass
  mixins: [State]
  render: ->
    <article className="nav_menu">
      <Link to="clubhouse" className="img">
        <figure>
          <img src="img/icons/golf_nav.png" />
        </figure>
      </Link>
      <Link to="user.friends" params={@getParams()}>Friends</Link>
      <Link to="user.groups" params={@getParams()}>Groups</Link>
      <Link to="user.courses" params={@getParams()}>Courses</Link>
      <Link to="user.about" params={@getParams()}>About</Link>
      <Link to="user.photos" params={@getParams()}>Photos</Link>
    </article>
