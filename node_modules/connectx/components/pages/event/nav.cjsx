
# @cjsx React.DOM

React = require 'react'
{Link, State} = require 'react-router'
Icon = require 'connectx/components/icon'

module.exports = React.createClass
  mixins: [State]
  render: ->
    <article className="nav_menu">
      <Link to="clubhouse" className="img">
        <figure>
          <img src="img/icons/golf_nav.png" />
        </figure>
      </Link>
      <Link to="event" params={@getParams()}><Icon fa-user /> Profile</Link>
      <Link to="event.about" params={@getParams()}>About</Link>
      <Link to="event.attending" params={@getParams()}>Attending</Link>
      <Link to="event.invite" params={@getParams()}>Invite</Link>
      <Link to="event.times" params={@getParams()}>Tee Times</Link>
      <Link to="event.results" params={@getParams()}>Results</Link>
      <Link to="event.photos" params={@getParams()}>Photos</Link>
      <Link to="event.settings" params={@getParams()}><Icon fa-cog /> Settings</Link>
    </article>
