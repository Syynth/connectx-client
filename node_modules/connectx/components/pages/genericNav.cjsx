React = require 'react'
{Link} = require 'react-router'
Icon = require 'connectx/components/icon'
{CurrentUserStore} = require 'connectx/stores'

module.exports = React.createClass
  render: ->
    <article className="nav_menu">
      {
        if CurrentUserStore.isLoggedIn()
          <Link to="clubhouse" className="img">
            <figure><img src="img/icons/golf_nav.png" /></figure>
          </Link>
      }
      <Link to="home"><Icon fa-calendar /> Home</Link>
    </article>
