
# @cjsx React.DOM

React = require 'react'
{Link, State} = require 'react-router'
Icon = require 'connectx/components/icon'

module.exports = React.createClass
  mixins: [State]
  render: ->
    <article className="nav_menu">
      <Link to="clubhouse" className="img">
        <figure>
          <img src="img/icons/golf_nav.png" />
        </figure>
      </Link>
      <Link to="course" params={@getParams()}><Icon fa-user /> Profile</Link>
      <Link to="course.about" params={@getParams()}>Friends</Link>
      <Link to="course.times" params={@getParams()}>Courses</Link>
      <Link to="course.friends" params={@getParams()}>Courses</Link>
      <Link to="course.results" params={@getParams()}>About</Link>
      <Link to="course.photos" params={@getParams()}>Photos</Link>
      <Link to="course.settings" params={@getParams()}>Settings</Link>
    </article>
