module.exports =
  getImageUrl: (imageId) -> 'http://placehold.it/1000x1000'
  getCoverPhoto: (entity) ->
    if entity?.coverImage?
      entity.coverImage
    else
      'img/defaults/header_bg.jpg'
  getProfilePhoto: (entity) ->
    if entity?.profileImage?
      entity.profileImage
    else
      'img/defaults/profile_img.jpg'
