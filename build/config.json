{
  "js": {
    "in": "./index.coffee",
    "out": "final.js",
    "dir": "./bin"
  },
  "img": {
    "in": "./res/img/**/*",
    "out": "./bin/img"
  },
  "server": {
    "root": "./bin/index.html",
    "url": "http://localhost:8000"
  },
  "style": {
    "in": [
      "./res/bower_components/.connectx-temp/libs.css",
      "./res/bower_components/.connectx-temp/master.css"
    ],
    "out": "final.css",
    "dir": "./bin"
  },
  "csslibs": {
    "in": [
      "./res/bower_components/bootstrap/dist/css/bootstrap.min.css",
      "./res/bower_components/font-awesome/css/font-awesome.min.css"
    ],
    "out": "libs.css",
    "dir": "./res/bower_components/.connectx-temp"
  }
}
